<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.georgialways.scavenger">

    <uses-feature
        android:required="true"
        android:name="org.cyanogenmod.theme" />

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="23" />

    <meta-data
        android:name="org.cyanogenmod.theme.name"
        android:value="@string/theme_name" />
    <meta-data
        android:name="org.cyanogenmod.theme.author"
        android:value="@string/theme_author" />
    <meta-data
        android:name="org.cyanogenmod.theme.email"
        android:value="@string/theme_email" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/theme_name"
        android:supportsRtl="true" />

</manifest>
